@model EntityLayer.Concrete.Contact 
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}
<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Correcting Subcontact Information</h4>

            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label for="exampleInputName1">Id</label>
                    <input type="text" asp-for="ContactId" class="form-control" id="exampleInputName1" placeholder="Id">
                </div>
                <div class="form-group">
                    <label for="exampleInputName1">Phone</label>
                    <input type="text" asp-for="Phone" class="form-control" id="exampleInputName1" placeholder="Phone">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword4">Mail</label>
                    <input type="text" asp-for="Mail" class="form-control" id="exampleInputName1" placeholder="Mail">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword4">Adress</label>
                    <input type="text" asp-for="Adress" class="form-control" id="exampleInputName1" placeholder="Mail">
                </div>

                <button type="submit" class="btn btn-primary mr-2">Submit</button>
                <button class="btn btn-light">Cancel</button>
            </form>

        </div>
    </div>
</div>


