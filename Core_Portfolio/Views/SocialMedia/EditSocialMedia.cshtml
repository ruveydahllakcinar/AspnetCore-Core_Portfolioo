@model EntityLayer.Concrete.SocialMedia
@{
    ViewData["Title"] = "EditSocialMedia";
    Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
}
<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Update Social Media Accounts</h4>

            <form class="forms-sample" method="post">
                <div class="form-group">
                    <label for="exampleInputName1">Social Media Id</label>
                    <input type="text" asp-for="SocialMediaId" class="form-control" id="exampleInputName1" placeholder="Social Media Id">
                </div>
                <div class="form-group">
                    <label for="exampleInputName1">Social Media Name</label>
                    <input type="text" asp-for="Name" class="form-control" id="exampleInputName1" placeholder="Social Media Name">
                </div>
                <div class="form-group">
                    <label for="exampleInputName1">Social Media Url</label>
                    <input type="text" asp-for="Url" class="form-control" id="exampleInputName1" placeholder="Social Media Url">
                </div>
                <div class="form-group">
                    <label for="exampleInputPassword4">Icon</label>
                    <input type="text" asp-for="Icon" class="form-control" rows="5" id="exampleInputName1" placeholder="Icon">
                </div>

                <button type="submit" class="btn btn-primary mr-2">Submit</button>
                <button class="btn btn-light">Cancel</button>
            </form>

        </div>
    </div>
</div>

